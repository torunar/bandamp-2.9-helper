{
  "manifest_version": 2,
  "name": "Bandamp 2.9 helper",
  "version": "2.9.2",
  "author": "Mikhail Shchekotov",
  "developer": {
    "name": "Mikhail Shchekotov",
    "url": "https://github.com/torunar"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "bandamp-29-helper@torunar.github.io"
